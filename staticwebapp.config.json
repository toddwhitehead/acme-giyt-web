{
  "routes": [],
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["/images/*.{png,jpg,gif,ico}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".xml": "application/xml"
  }
}
